#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/larinnimalheiros/Development/705digital/node_modules/.pnpm/bulma-css-vars@0.8.0_bulma@0.9.4/node_modules/bulma-css-vars/bin/node_modules:/Users/larinnimalheiros/Development/705digital/node_modules/.pnpm/bulma-css-vars@0.8.0_bulma@0.9.4/node_modules/bulma-css-vars/node_modules:/Users/larinnimalheiros/Development/705digital/node_modules/.pnpm/bulma-css-vars@0.8.0_bulma@0.9.4/node_modules:/Users/larinnimalheiros/Development/705digital/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/larinnimalheiros/Development/705digital/node_modules/.pnpm/bulma-css-vars@0.8.0_bulma@0.9.4/node_modules/bulma-css-vars/bin/node_modules:/Users/larinnimalheiros/Development/705digital/node_modules/.pnpm/bulma-css-vars@0.8.0_bulma@0.9.4/node_modules/bulma-css-vars/node_modules:/Users/larinnimalheiros/Development/705digital/node_modules/.pnpm/bulma-css-vars@0.8.0_bulma@0.9.4/node_modules:/Users/larinnimalheiros/Development/705digital/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../bulma-css-vars/bin/bulma-css-vars.js" "$@"
else
  exec node  "$basedir/../bulma-css-vars/bin/bulma-css-vars.js" "$@"
fi
